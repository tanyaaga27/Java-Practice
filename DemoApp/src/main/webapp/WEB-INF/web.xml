<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
  <welcome-file-list>
  <welcome-file>home.html</welcome-file>
  </welcome-file-list>
  
  <error-page>
  <exception-type>java.lang.NumberFormatException</exception-type>
  <location>/ExceptionPage.html</location>
  </error-page>
  

    
  <servlet>
  <servlet-name>abc</servlet-name>
  <servlet-class>com.ServletPractice.AddServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>abc</servlet-name>
  <url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>pqr</servlet-name>
  <servlet-class>com.ServletPractice.SqServlet</servlet-class>
  
  <init-param>
  <param-name>name</param-name>
  <param-value>Priya</param-value>
  </init-param>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>pqr</servlet-name>
  <url-pattern>/sq</url-pattern>
  
  </servlet-mapping>
  
  <context-param>
  <param-name>name</param-name>
  <param-value>Tanya</param-value>
  </context-param>
  
  <context-param>
  <param-name>Job Profile</param-name>
  <param-value>Jr. Software Engineer</param-value>
  </context-param>
  
  </web-app>
  